<odoo>
    <record id="view_res_users_form_inherit_properties" model="ir.ui.view">
        <field name="name">res.users.form.inherit.properties</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='account_security']" position="after">
                <page string="Real Estate Properties">
                    <field name="property_ids" widget="many2many_list" />
                </page>
            </xpath>
        </field>
    </record>
</odoo>
